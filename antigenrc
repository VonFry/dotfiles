antigen use prezto

antigen bundles <<EOBUNDLES
    environment
    archive
    autosuggestions
    completion
    directory
    docker
    git
    gpg
    haskell
    helper
    history-substring-search
    history
    prompt
    spectrum
    syntax-highlighting
    terminal
    utility

    paulirish/git-open
    mafredri/zsh-async
EOBUNDLES

if [[ $(uname) = 'Darwin' ]]; then
    antigen bundle bundle
    antigen bundle osx
fi

antigen theme geometry-zsh/geometry

antigen apply
